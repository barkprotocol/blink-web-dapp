# Database connection strings
POSTGRES_URL="postgres://<username>:<password>@<host>:<port>/<database>?sslmode=require&supa=<your_supabase_pool>"
POSTGRES_PRISMA_URL="postgres://<username>:<password>@<host>:<port>/<database>?sslmode=require&supa=<your_supabase_pool>"
POSTGRES_URL_NON_POOLING="postgres://<username>:<password>@<host>:<port>/<database>?sslmode=require"

# Supabase configurations
SUPABASE_URL="https://<your_supabase_project>.supabase.co"
NEXT_PUBLIC_SUPABASE_URL="https://<your_supabase_project>.supabase.co"
SUPABASE_JWT_SECRET="<your_jwt_secret>"
SUPABASE_SERVICE_ROLE_KEY="<your_service_role_key>"
SUPABASE_ANON_KEY="<your_anon_key>"

# PostgreSQL credentials
POSTGRES_USER="<your_postgres_username>"
POSTGRES_PASSWORD="<your_postgres_password>"
POSTGRES_DATABASE="<your_postgres_database>"
POSTGRES_HOST="<your_postgres_host>"

# Solana configurations
# Available networks: 'mainnet', 'devnet', 'testnet'
NEXT_PUBLIC_SOLANA_NETWORK="devnet"
NEXT_PUBLIC_SOLANA_RPC_URL="https://api.devnet.solana.com" # For devnet
# NEXT_PUBLIC_SOLANA_RPC_URL="https://api.mainnet-beta.solana.com" # Uncomment for mainnet

# Token Program ID for Solana tokens
TOKEN_PROGRAM_ID="<your_token_program_id>"

# NFT Program ID for Solana NFTs
NFT_PROGRAM_ID="<your_nft_program_id>"

# Default wallet address for transaction fees or app-specific operations
DEFAULT_WALLET_ADDRESS="<your_default_wallet_address>"

# Helius API for Solana indexing and RPC services
HELIUS_API_URL="https://rpc.helius.xyz/?api-key=<your_helius_api_key>"

# Error Tracking and Monitoring
SENTRY_DSN="<your_sentry_dsn>"

# Stripe configurations
STRIPE_SECRET_KEY="<your_stripe_secret_key>"
STRIPE_WEBHOOK_SECRET="<your_stripe_webhook_secret>"

# Application settings
BASE_URL="http://localhost:3000"
AUTH_SECRET="<your_auth_secret>"

# CORS settings
ALLOWED_ORIGINS="<comma_separated_allowed_origins>"

